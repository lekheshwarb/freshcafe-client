{{yield}}
<div class="d-flex justify-content-evenly my-3 mx-5">

  <div class="d-flex fw-semibold fs-6 my-1" style="color: rgb(139, 139, 139);">
    <i class="bi bi-calendar-date-fill"></i>&nbsp; From Date:&nbsp;
    {{ember-flatpickr
      class="mx-3"
      placeholder="Choose end date"
      onChange=(action (mut dateFrom))
    }}
  </div>

  <div class="d-flex fw-semibold fs-6 my-1" style="color: rgb(139, 139, 139);">
    <i class="bi bi-calendar-date-fill"></i>&nbsp; To Date:&nbsp;
    {{ember-flatpickr
      class="mx-3"
      placeholder="Choose end date"
      onChange=(action (mut dateTo))
    }}
  </div>

  <div class="d-flex fw-semibold fs-6 my-1" style="color: rgb(139, 139, 139);">
    <i class="bi bi-person-fill"></i>&nbsp; User
    <select
      class="mx-3 col form-select form-select-sm"
      aria-label=".form-select-sm example"
      value={{this.userFilter}}
      onchange={{action (mut userFilter) value="target.value"}}
    >
      <option selected value="null">All</option>
      {{#each usersList as |user|}}
        <option value={{user.name}}>{{user.name}}</option>
      {{/each}}
    </select>

  </div>

</div>
